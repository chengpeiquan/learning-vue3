    "@web-analytics/vue": "^0.2.2"
  },
  "devDependencies": {
    "@bassist/eslint-config": "^0.1.2",
    "@bassist/tsconfig": "^0.1.1",
    "@types/node": "^20.4.8",
    "@withtypes/md5": "^0.1.1",
    "eslint": "^9.22.0",
    "gitalk": "^1.8.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "vite-plugin-banner": "^0.7.0",
    "vitepress": "1.0.0-beta.7",
    "vue": "^3.3.4"
  }
